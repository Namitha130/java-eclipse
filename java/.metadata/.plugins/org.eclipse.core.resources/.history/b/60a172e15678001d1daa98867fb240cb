package com;

public class example {
	public static void main(String[] args) 
	{

		int a=5;
		int b=4; 
		boolean c= a>b || a>b;
		System.out.println(c);
		boolean d= a>b && a>b;
				// ternary operators
		String answer =(a>b) ? "hi":"bye"; 
		System.out.println(answer);
		 
		int x=0;
		if(x>=0)
		{
			System.out.println("+ve number");
		}
		else {
			System.out.println("-ve numbers");
		}
	}

}