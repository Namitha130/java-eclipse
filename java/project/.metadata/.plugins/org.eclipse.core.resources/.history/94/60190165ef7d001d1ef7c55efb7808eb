package com;

public class switchOperator {
   
public static void main(String[] args) {
int num1 = 100;
	
	switch(num1 % 2)
    {
        
     case 0: 
           System.out.println("Number is even");
            break;

       	    case 1: 
            System.out.println("Number is odd");
            
       	 }; 
	}
}