package com;

public class dec13 {
	public static void display(char alpha) {
		
		for (char i = 'a'; i <= alpha ; i++) 
		{
			for (char j = 'a'; j <= alpha ; j++) 
			{
				System.out.print(j + " ");
			}
			System.out.println(" ");
		}
	}
	public static void main(String[] args) {
		
		display(e);
	}

}
